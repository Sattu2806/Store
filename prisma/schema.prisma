// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Project {
  id       Int      @id @default(autoincrement())
  Discipline String
  Area       String
  Date       DateTime
  Excavation Float
  FormWork   Float
  Rebar      Float
  Concrete  Float
}

model MonthlyData {
  id     Int    @id @default(autoincrement())
  Type   String
  Month  String
  Value  Int
}

model TradeData {
  id     Int      @id @default(autoincrement())
  Type   String
  Trade  String
  Month  String
  Value  Int
}